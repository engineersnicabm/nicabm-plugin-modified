{
  "name": "nicabm/nicabm-utility-plugin",
  "description": "Holds the core functionality of nicabm.com.",
  "version": "0.2.0",
  "keywords": [
    "wordpress"
  ],
  "type": "wordpress-plugin",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Tim Jensen",
      "homepage": "https://www.timjensen.us/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "webdevstudios/cmb2": "^2.2",
    "composer/installers": "~1.0",
    "timothyjensen/acf-field-group-values": "^2.0"
	"myclabs/deep-copy": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "extra": {
    "installer-paths": {
      "vendor/CMB2/": [
        "webdevstudios/cmb2"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "NICABM\\Utility\\": "src/"
    }
  }
}
